void setup (){
size(400,400);
background (190);
//color de figura
fill (0,255,0);
//hace un rectangulo
rect (260,50,100,100);
//el color de relleno de mi rectangulo
fill (0,0,255);
//hace un elipse
ellipse (310,310,100,100);
//color del relleno de la elipse 
fill (0);
//tamaNio de texto q imprime en pantalla
textSize (20);
text ("Esto es un cuadrado verde:",12,100);
textSize (20);
text ("Esto es una ellipse azul:",12,300);
}
